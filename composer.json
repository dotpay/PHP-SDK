{
    "name": "dotpay/php-sdk",
    "version": "1.0.16",
    "description": "Universal API to creating payment gateways for e-commerce thanks to Dotpay services",
    "keywords": [
        "dotpay",
        "payment",
        "ecommerce",
        "api"
    ],
    "homepage": "https://dotpay.pl",
    "license": "MIT",
    "authors": [{
        "name": "Dotpay sp. z o.o.",
        "email": "tech@dotpay.pl",
        "role": "Developer"
    }],
    "require": {
        "php": ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "5.5.*",
        "phpunit/php-invoker": "*",
        "phpunit/dbunit": ">=1.2",
        "mockery/mockery": "dev-master",
        "phpdocumentor/phpdocumentor":"2.*"
    },
    "autoload": {
        "psr-4": {
            "Dotpay\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    }
}
